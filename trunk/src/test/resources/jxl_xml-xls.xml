<workbook>
	<sheet name="wiz">
        <column-width column="1" width="20000"/>
        <merge region="1,1,1,2" />
		<row placement="0">
			<cell placement="0" style="align:center; text-decoration: bold; valign: center ; border: dotted">
				<text>My Heading</text>
			</cell>
		</row>
		<row placement="2">
			<cell placement="0" style="background:aqua; color: dark_red; align: center ; border : double">
				<text>Hello World!</text>
			</cell>
			<cell placement="2">
				<number>3</number>
			</cell>
			<cell placement="4">
				<date format="dd-MM-yyyy">12-12-2005</date>
			</cell>
		</row>
		<row placement="3">
			<cell placement="2">
				<number>4</number>
			</cell>
		</row>
		<row placement="4">
			<cell placement="2">
				<number>4</number>
			</cell>
		</row>
		<row placement="5">
			<cell placement="2" style="border: dotted; color:grey_50_percent">
				<formula>sum(C3:C5)</formula>
			</cell>
		</row>
	</sheet>
	<sheet name="dada">
		<row placement="1">
			<cell placement="1">
				<text>Subhash</text>
			</cell>
			<cell placement="2">
				<boolean>true</boolean>
			</cell>
			<cell placement="3">
				<calendar format="dd-MM-yyyy">12-12-2005</calendar>
			</cell>
		</row>
	</sheet>
	<!-- Sheet without name -->
	<sheet>
		<row placement="1">
			<cell placement="1">
				<text>Sheet with default name!</text>
			</cell>
		</row>
	</sheet>
</workbook>
